var period = 10s
var every = 10s
stream
    // Select just the cpu measurement from our example database.
    |from()
        .measurement('cpu')
    |window()
       .period(period)
       .every(every)
    |alert()
        .message('CPUUUU usage alert')
        .crit(lambda: "usage_idle" <  variable)
        // Whenever we get an alert write it to a file.
        .log('/tmp/alerts.log')
        //.email('rdammala@ufl.edu')
